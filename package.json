{
  "name": "leos-law",
  "version": "1.2.2",
  "description": "A blazing fast typed javascript library Inspired by Leonardo Dicaprio that checks if you should break up with your partner based on her age.",
  "scripts": {
    "dev": "tsup index.ts --minify --sourcemap --dts --format cjs,esm --watch",
    "build": "tsup index.ts --minify --dts --format cjs,esm"
  },
  "author": "Jack van der Bilt <contact@managing.software> (https://managing.software)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UltraCakeBakery/leos-law.git"
  },
  "bugs": {
    "url": "https://github.com/UltraCakeBakery/leos-law/issues"
  },
  "homepage": "https://github.com/UltraCakeBakery/leos-law#readme",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/jackvanderbilt"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.16",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "type": "module"
}
